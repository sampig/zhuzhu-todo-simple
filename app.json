{
  "expo": {
    "name": "zhuzhu To-do Simple",
    "description": "zhuzhu to-do application (the simple version).",
    "slug": "zhuzhuTodoSimple",
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/todo_icon.png",
    "splash": {
      "image": "./assets/todo_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/576b56e8-ad79-4750-b39c-b5f81df980c3"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "org.zhuzhu.todoSimple",
      "buildNumber": "1.0.1",
      "icon": "./assets/todo_icon_ios.png",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      },
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/todo_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "org.zhuzhu.todoSimple",
      "permissions": ["com.google.android.gms.permission.AD_ID"]
    },
    "web": {
      "favicon": "./assets/todo_favicon.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "576b56e8-ad79-4750-b39c-b5f81df980c3"
      }
    },
    "owner": "sampig",
    "runtimeVersion": "1.0.4"
  }
}
